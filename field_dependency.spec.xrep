`{`@eq`(`'get_required_field_names`'`,`$section`)\
   // [(start field_dependency)]
   if( needs_field_value( "`{`$arg_field`}", dependents ) )
   {
      dependents.insert( "`{`$arg_sfield`}" );

      if( ( required_transients && is_field_transient( e_field_id_`{`$arg_sfield`} ) )
       || ( !required_transients && !is_field_transient( e_field_id_`{`$arg_sfield`} ) ) )
         names.insert( "`{`$arg_sfield`}" );
   }
   // [(finish field_dependency)]
`}
