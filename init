if [ $# -lt 1 ]; then
 echo Usage: init [model name] [[app name]]
else
 export CIYAM_MODEL=$1
 export CIYAM_STORAGE=$1
 if [ $# -gt 1 ]; then
  export CIYAM_STORAGE=$2
 fi
 rm -f $CIYAM_STORAGE.log $CIYAM_STORAGE.dat $CIYAM_STORAGE.idx $CIYAM_STORAGE.hdr >/dev/null
 ./ciyam_server -quiet &
 sleep 0.3
 ./ciyam_client -quiet -no_prompt < init.cin
 touch ciyam_server.stop
fi
