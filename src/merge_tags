if [ $# -lt 1 ]; then
 echo "Usage: merge_tags [tag name(s) and/or prefix] [[tag name]]"
else
 ./ciyam_client -echo -quiet -no_prompt -no_stderr -exec="<merge_tags.cin $1 $2"
fi
