if [ $# -lt 1 ]; then
 echo Usage: import_files [start path]
else
 echo "<<import_files.cin $1" >~import_files
 ./ciyam_client -echo -quiet -no_prompt -no_stderr <~import_files
 rm ~import_files
fi
