if [ $# -lt 2 ]; then
 echo "Usage: tree_list_files [tree tag] [trunk name] [[branch expression]]"
else
 ./ciyam_client -echo -quiet -no_prompt -no_stderr -exec="<tree_list_files.cin $1 $2 $3"
fi
