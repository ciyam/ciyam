NUM_ARGS=0
@ifdef $1
NUM_ARGS=@$NUM_ARGS+1
@endif
@ifdef $2
NUM_ARGS=@$NUM_ARGS+1
@endif
@ifdef $3
NUM_ARGS=@$NUM_ARGS+1
@endif
;
@ifneq "$NUM_ARGS" "3"
#NOTE: This script requires a tree tag, branch expr and a variable name.
@else
;
TREE=$1
BRANCH=$2
VARIABLE=$3
;
DEPTH=3
;
.session_variable @deque ""
LINES=@repstr:\:/\n:$TREE
.session_variable @deque "~$LINES"
.session_variable @deque size
DEPTH=@$DEPTH+$OUTPUT
;
.session_variable @deque ""
.file_info -recurse -d=-$DEPTH -p=Variables:$BRANCH:$VARIABLE $TREE
;
@ifdef $OUTPUT
#%OUTPUT:-1%
@endif
;
@endif
