if [ $# -lt 1 ]; then
 echo "Usage: generate_next_block [<identity> else \"-hub\", \"-backup\" or \"-shared\" [[seed] [[tree tag]]]]"
else
 ./ciyam_client -echo -quiet -no_prompt -no_stderr -exec="<generate_next_block.cin $*"
fi
