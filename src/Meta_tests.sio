<sio/>
 <groups/>
  <group/>
   <name>test_meta
   <tests/>
    <test/>
     <name>1
     <description>Test installation of the Standard and User packages.
     <test_step/>
      <name>*init*
      <exec>*ciyam_server -quiet
      <input>false
      <output>none
     </test_step>
     <test_step/>
      <name>a
      <exec>ciyam_client -echo -quiet -no_stderr
      <input>true
      <output>generate
     </test_step>
    </test>
    <test/>
     <name>2
     <description>Test storage backup and restore.
     <kill>ciyam.backup.bun.gz
     <test_step/>
      <name>a
      <exec>ciyam_client -echo -quiet -no_stderr
      <input>true
      <output>generate
     </test_step>
     <test_step/>
      <name>b
      <exec>ciyam_client -echo -quiet -no_stderr
      <input>true
      <output>generate
     </test_step>
     <test_step/>
      <name>c
      <exec>ciyam_client -echo -quiet -no_stderr
      <input>true
      <output>generate
     </test_step>
    </test>
    <test/>
     <name>3
     <description>Test removing all packages and deleting Model.
     <test_step/>
      <name>a
      <exec>ciyam_client -echo -quiet -no_stderr
      <input>true
      <output>generate
     </test_step>
     <test_step/>
      <name>*term*
      <exec>$touch ciyam_server.stop
      <input>false
      <output>none
     </test_step>
    </test>
   </tests>
  </group>
 </groups>
</sio>
