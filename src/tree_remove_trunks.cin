NUM_ARGS=0
@ifdef $1
NUM_ARGS=@$NUM_ARGS+1
@endif
@ifdef $2
NUM_ARGS=@$NUM_ARGS+1
@endif
;
@ifneq "$NUM_ARGS" "2"
#NOTE: This script requires a tree tag and one or more trunk items.
@else
;
TREE=$1
TRUNKS=$2
;
.file_list -d=$TRUNKS -sort $TREE !$TREE
;
@endif
