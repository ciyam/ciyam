 echo "Usage: tree_store_variable [tree tag] [branch expr] [variable name] [[variable data]]"
else
 ./ciyam_client -echo -quiet -no_prompt -no_stderr -exec="<tree_store_variable.cin $1 $2 $3 \"$4\""
fi
