`{`@eq`(`'to_store`'`,`$section`)\
   // [(start field_cond_concatenate)]
   if( !get_obj( ).get_key( ).empty( ) )
   {
      string value( get_obj( ).`{`$arg_dest_field`}( ) );

      if( get_obj( ).`{`$arg_src_field`}( ) == `{`$arg_tvalue`} )
      {
         if( !value.empty( ) )
            value += ',';
         value += "`{`$arg_dvalue`}";

         get_obj( ).`{`$arg_dest_field`}( value );
      }
   }
   // [(finish field_cond_concatenate)]
`}
